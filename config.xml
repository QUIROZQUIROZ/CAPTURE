<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
 
  <name>12.1</name>
  <description>
      Esto es una pagina en PhoneGap
  </description>
  <author email="EDUARDO@HOTMAIL.COM" href="http://cordova.io"> JESUS EDUARDO </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-dialogs"/>
  <plugin name="cordova-plugin-device"/>
  <plugin name="cordova-plugin-device-motion"/>
  <plugin name="cordova-plugin-camera"/>
   <plugin name=" cordova-plugin-media-capture"/>
    <plugin name=" cordova-plugin-media"/>
   
    
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>
